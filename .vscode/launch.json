    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch index",
            "program": "${workspaceFolder}/index.js"
        },
        {
          "type": "node",
          "request": "launch",
          "name": "Layout inner",
          "program": "${workspaceFolder}/scripts/layout/layoutInner.js"
        },
        {
          "type": "node",
          "request": "launch",
          "name": "Download needed height data",
          "program": "${workspaceFolder}/scripts/tiles/height/download.js"
        },
        {
          "type": "node",
          "request": "launch",
          "name": "Produce height tiles",
          "program": "${workspaceFolder}/scripts/tiles/height/produceTiles.js"
        }
    ]
}
